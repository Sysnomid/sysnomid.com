<meta charset="utf-8">
<link rel="stylesheet" href="/assets/css/main.css">
<meta name="viewport" content="width=device-width, initial-scale=1.0">


<!-- HTML Meta Tags -->
<title>{{ title }} - {{ data.name }}</title>
<meta name="description" content="{{ excerpt }}">

<!-- Facebook Meta Tags -->
<meta property="og:url" content="https://sysnomid.com">
<meta property="og:type" content="website">
<meta property="og:title" content="{{ title }} - {{ data.name }}">
<meta property="og:description" content="{{ excerpt }}">
<meta property="og:image" content="https://sysnomid.com/assets/favicon.png">

<!-- Twitter Meta Tags -->
<meta name="twitter:card" content="summary_large_image">
<meta property="twitter:domain" content="sysnomid.com">
<meta property="twitter:url" content="https://sysnomid.com">
<meta name="twitter:title" content="{{ title }} - {{ data.name }}">
<meta name="twitter:description" content="{{ excerpt }}">
<meta name="twitter:image" content="/assets/favicon.png">

    
<link rel="shortcut icon" href="/assets/favicon.png" type="image/x-icon">
<link rel="apple-touch-icon" href="/apple-touch-icon.png">

<link rel="canonical" href="{{ data.url }}{{ page.url }}" />
<link rel="preconnect" href="https://fonts.gstatic.com">
<link href="https://fonts.googleapis.com/css2?family=Merriweather:wght@300&family=Inter:wght@{{data.weight}}&display=swap" rel="stylesheet">
<title> {{ title }} - {{ data.name }}</title>